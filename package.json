{
  "name": "store",
  "version": "0.0.1",
  "description": "store",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "test": "set NODE_ENV=test&& mocha src --recursive --timeout 10000 --compilers js:babel-register --require babel-polyfill"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0
    }
  },
  "repository": "einfachIOTA/store",
  "author": "huhn <huhn@einfachIOTA.de>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "dynamic-html-pdf": "^1.0.1",
    "express": "^4.13.3",
    "iota-payment": "0.0.31",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.8.0",
    "nodemailer": "^6.3.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "paypal-rest-sdk": "^1.8.1",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^4.18.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  }
}
